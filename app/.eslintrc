{
  "extends": "../.eslintrc",
  "env": {
    "node": false,
    "browser": true
  },
  "globals": {
    "angular": true,
    "ionic": true,
    "localforage": true
  },
  "rules": {
    "no-console": 0,
    "eqeqeq": 0,
    "no-use-before-define": 0,
    "no-unused-vars" : 0,
    "no-undef": 0,
    "no-redeclare" : 0,
    "no-cond-assign" : 0,
    "no-empty" : 0,
    "no-fallthrough" : 0,
    "no-control-regex" : 0
  }
}
